{
	"manifest_version": 2,

	"name": "Calendar Enhancements",
	"description": "Adds some productivity enhancements to Google Calendar",
	"version": "1.0.0",
	
	"background": {
		"scripts": ["background.js"]
	},
		"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": [ "tabs", "https://www.googleapis.com/", "https://www.google.com/calendar/render" ],

	"browser_action": {
		"default_icon": "icon.png"//,
		//"default_popup": "popup.html"
	}
}